										{
											"name": "SITE_ID",
											"value": {
												"value": "@item().SiteID",
												"type": "Expression"
											}
										},
										{
											"name": "SITE_NAME",
											"value": {
												"value": "@item().SiteName",
												"type": "Expression"
											}
										}
									],
									"httpRequestTimeout": "00:01:40",
									"requestInterval": "00.00:00:00.010",
