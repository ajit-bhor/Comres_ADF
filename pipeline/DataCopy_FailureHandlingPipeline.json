								"sink": {
									"name": "SiteID",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "['siteName']"
								},
								"sink": {
									"name": "SiteName",
									"type": "String"
								}
							}
						],
						"collectionReference": "$['content']"
					}
