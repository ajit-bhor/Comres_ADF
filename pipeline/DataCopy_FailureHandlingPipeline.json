											"sink": {
												"name": "DeviceID",
												"type": "Int32"
											}
										},
										{
											"source": {
												"path": "['deviceName']"
											},
											"sink": {
												"name": "DeviceName",
												"type": "String"
											}
										},
										{
											"source": {
												"path": "$['SITE_ID']"
											},
											"sink": {
												"name": "SiteID",
												"type": "Int32"
											}
										},
										{
											"source": {
												"path": "$['SITE_NAME']"
											},
											"sink": {
												"name": "SiteName",
												"type": "String"
											}
										}
									],
									"collectionReference": "$['content']"
								}
							},
							"inputs": [
								{
									"referenceName": "DevicesApiRestDataSource",
									"type": "DatasetReference",
									"parameters": {
										"siteId": {
											"value": "@item().SiteID",
											"type": "Expression"
										}
									}
								}
							],
							"outputs": [
								{
									"referenceName": "SqlServerSiteDeviceDataSet",
									"type": "DatasetReference"
								}
