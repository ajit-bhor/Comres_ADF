					"dataset": {
						"referenceName": "SqlServerSiteIdDataSet",
						"type": "DatasetReference"
					},
					"firstRowOnly": false
				}
			},
			{
				"name": "Set Failure",
				"type": "SetVariable",
				"dependsOn": [
					{
						"activity": "SiteApiCopy",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"userProperties": []
			}
		],
		"parameters": {
			"Failure": {
				"type": "string",
				"defaultValue": "0"
			}
		},
		"annotations": []
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}