{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Comres-Site-Supervisor-ADF"
		},
		"RESTTokenLinkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'RESTTokenLinkedservice'"
		},
		"SQLServerLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLServerLinkedService'"
		},
		"SqlServerSiteDevicesLinkedService_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServerSiteDevicesLinkedService'"
		},
		"DeviceApiLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "HTTP://192.168.114.134/inventory/"
		},
		"DevicesApiLinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://192.168.114.134/inventory/"
		},
		"RESTTokenLinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://192.168.114.134/uaa/auth"
		},
		"RESTTokenLinkedservice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "administrator"
		},
		"SiteSupervisorLinedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "HTTP://192.168.114.134/inventory/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"RESTTokenLinkedservice_password": {
						"value": "[parameters('RESTTokenLinkedservice_password')]"
					},
					"SQLServerLinkedService_connectionString": {
						"value": "[parameters('SQLServerLinkedService_connectionString')]"
					},
					"SqlServerSiteDevicesLinkedService_connectionString": {
						"value": "[parameters('SqlServerSiteDevicesLinkedService_connectionString')]"
					},
					"RESTTokenLinkedservice_properties_typeProperties_url": {
						"value": "[parameters('RESTTokenLinkedservice_properties_typeProperties_url')]"
					},
					"RESTTokenLinkedservice_properties_typeProperties_userName": {
						"value": "[parameters('RESTTokenLinkedservice_properties_typeProperties_userName')]"
					},
					"DeviceApiLinkedService_properties_typeProperties_url": {
						"value": "[parameters('DeviceApiLinkedService_properties_typeProperties_url')]"
					},
					"DevicesApiLinkedservice_properties_typeProperties_url": {
						"value": "[parameters('DevicesApiLinkedservice_properties_typeProperties_url')]"
					},
					"SiteSupervisorLinedService_properties_typeProperties_url": {
						"value": "[parameters('SiteSupervisorLinedService_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}